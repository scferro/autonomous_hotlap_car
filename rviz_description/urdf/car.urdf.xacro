<robot name="car" xmlns:xacro="http://www.ros.org/wiki/xacro">
      <link name="base_link">
        <inertial>
          <origin xyz="0 -0 0.08" rpy="0 -0 0"/>
          <mass value="1.14395" />
          <inertia
            ixx="0.126164"
            ixy="0"
            ixz="0"
            iyy="0.416519"
            iyz="0"
            izz="0.481014"
          />
        </inertial>
        <visual>
          <origin xyz="-0 0 0.09" rpy="0 -0 0"/>
          <geometry>
            <box size="0.34 0.1 0.1" />
          </geometry>
          <material name="pla">
             <color rgba="0.5 0.1 0.5 1.0" />
          </material>
        </visual>
        <collision>
          <origin xyz="-0 0 0.08" rpy="0 -0 0"/>
          <geometry>
            <box size="0.35 0.1 0.1" />
          </geometry>
        </collision>
      </link>

      <link name="base_footprint" />
      <joint name="base_joint" type="fixed">
             <parent link="base_link"/>
             <child link="base_footprint"/>
             <origin xyz="0.0 0.0 -0.15" rpy="0.0 0.0 0.0" />
     </joint>

      <link name="base_laser">
        <inertial>
          <mass value="0.1"/>
          <inertia
            ixx="0.000166667"
            iyy="0.000166667"
            izz="0.000166667"
            ixy="0.0"
            ixz="0.0"
            iyz="0.0"
          />
        </inertial>
        <collision>
          <geometry>
            <cylinder radius="0.05" length="0.04" />
          </geometry>
        </collision>
        <visual>
          <geometry>
            <cylinder radius="0.05" length="0.04" />
          </geometry>
          <material name="pla_black">
             <color rgba="0.3 0.3 0.3 1.0" />
          </material>
        </visual>
      </link>

      <link name="rear_left_wheel">
        <inertial>
          <mass value="2.0" />
          <inertia
            ixx="0.145833"
            ixy="0"
            ixz="0"
            iyy="0.145833"
            iyz="0"
            izz="0.125"
          />
        </inertial>
        <visual>
          <geometry>
            <cylinder radius="0.054" length="0.043" />
          </geometry>
          <material name="rubber">
             <color rgba="0.1 0.1 0.1 1" />
          </material>
        </visual>
        <collision name='collision'>
          <geometry>
            <cylinder radius="0.054" length="0.043" />
          </geometry>
        </collision>
      </link>

      <link name="rear_right_wheel">
        <inertial>
          <mass value="2.0" />
          <inertia
            ixx="0.145833"
            ixy="0"
            ixz="0"
            iyy="0.145833"
            iyz="0"
            izz="0.125"
          />
        </inertial>
        <visual>
          <geometry>
            <cylinder radius="0.054" length="0.043" />
          </geometry>
          <material name="rubber" />
        </visual>
        <collision name='collision'>
          <geometry>
            <cylinder radius="0.054" length="0.043" />
          </geometry>
        </collision>
      </link>

      <link name="front_left_wheel">
        <inertial>
          <mass value="2.0" />
          <inertia
            ixx="0.145833"
            ixy="0"
            ixz="0"
            iyy="0.145833"
            iyz="0"
            izz="0.125"
          />
        </inertial>
        <visual>
          <geometry>
            <cylinder radius="0.054" length="0.043" />
          </geometry>
          <material name="rubber">
             <color rgba="1.0 1.0 1.0 1.0" />
          </material>
        </visual>
        <collision name='collision'>
          <geometry>
            <cylinder radius="0.054" length="0.043" />
          </geometry>
        </collision>
      </link>

      <link name="front_right_wheel">
        <inertial>
          <mass value="2.0" />
          <inertia
            ixx="0.145833"
            ixy="0"
            ixz="0"
            iyy="0.145833"
            iyz="0"
            izz="0.125"
          />
        </inertial>
        <visual>
          <geometry>
            <cylinder radius="0.054" length="0.043" />
          </geometry>
          <material name="rubber" />
        </visual>
        <collision name='collision'>
          <geometry>
            <cylinder radius="0.054" length="0.043" />
          </geometry>
        </collision>
      </link>

      <link name="front_left_hub">
        <inertial>
          <mass value="0.0001" />
          <inertia
            ixx="0.001"
            ixy="0"
            ixz="0"
            iyy="0.001"
            iyz="0"
            izz="0.001"
          />
        </inertial>
        <collision name='collision'>
          <geometry>
            <box size="0.05 0.08 0.05" />
          </geometry>
        </collision>
      </link>

      <link name="front_right_hub">
        <inertial>
          <mass value="0.0001" />
          <inertia
            ixx="0.001"
            ixy="0"
            ixz="0"
            iyy="0.001"
            iyz="0"
            izz="0.001"
          />
        </inertial>
        <collision name='collision'>
          <geometry>
            <box size="0.05 0.08 0.05" />
          </geometry>
        </collision>
      </link>


      <joint name="lidar_sensor_joint" type="fixed">
        <origin xyz="0.05 -0 0.16" rpy="0 0 0" />
        <parent link="base_link" />
        <child link="base_laser" />
      </joint>

      <joint name="rear_left_wheel_joint" type="continuous">
        <origin xyz="-0.14 0.13 0.054" rpy="-1.5707 0 0" />
        <parent link="base_link" />
        <child link="rear_left_wheel" />
        <axis xyz="0 0 1" />
      </joint>

      <joint name="rear_right_wheel_joint" type="continuous">
        <origin xyz="-0.14 -0.13 0.054" rpy="-1.5707 0 0" />
        <parent link="base_link" />
        <child link="rear_right_wheel" />
        <axis xyz="0 0 1" />
      </joint>

      <joint name="front_left_wheel_joint" type="continuous">
        <origin xyz="-0 0.02 0" rpy="-1.5707 0 0" />
        <parent link="front_left_hub" />
        <child link="front_left_wheel" />
        <axis xyz="0 0 1" />
      </joint>

      <joint name="front_right_wheel_joint" type="continuous">
        <origin xyz="-0 -0.02 0" rpy="-1.5707 0 0" />
        <parent link="front_right_hub" />
        <child link="front_right_wheel" />
        <axis xyz="0 0 1" />
      </joint>

      <joint name="front_left_hub_joint" type="revolute">
        <origin xyz="0.14 0.11 0.054" rpy="0 0 0" />
        <limit effort="1000" upper="0.5" lower="-0.5" velocity="100.0" />
        <parent link="base_link" />
        <child link="front_left_hub" />
        <axis xyz="0 0 1" />
      </joint>

      <joint name="front_right_hub_joint" type="revolute">
        <origin xyz="0.14 -0.11 0.054" rpy="0 0 0" />
        <limit effort="1000" upper="0.5" lower="-0.5" velocity="100.0" />
        <parent link="base_link" />
        <child link="front_right_hub" />
        <axis xyz="0 0 1" />
      </joint>

      <xacro:include filename="$(find rviz_description)/urdf/car.gazebo.xacro" />
</robot>
